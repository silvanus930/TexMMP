{
  "name": "TexMMP",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ios:pod": "cd ./ios && bundle exec pod update && bundle exec pod install --repo-update",
    "ios:pod-clean": "cd ./ios && bundle exec pod cache clean --all && rm -rf Pods",
    "ios:pod-update": "cd ./ios && bundle exec pod update && bundle exec pod install --repo-update",
    "ios:xcode": "cd ./ios && xcodebuild -project TexMMP.xcodeproj -target TexMMP -showBuildSettings",
    "android:clean": "cd android && ./gradlew clean && cd .. && rm -rf ./android/app/src/main/res/drawable-*/",
    "android:b": "react-native run-android --variant=release",
    "android:r": "cd android && ./gradlew assembleDebug && cd ..",
    "clear": "rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && yarn cache clean && yarn install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.71.0",
    "react-native-audio-recorder-player": "^3.5.3",
    "react-native-document-picker": "^8.2.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-image-picker": "^5.3.1",
    "react-native-image-sequence-2": "^0.9.1",
    "react-native-indicators": "^0.17.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-permissions": "^3.8.0",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.20.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-step-indicator": "^1.0.3",
    "react-native-svg": "^13.9.0",
    "react-native-typing-animation": "^0.1.7",
    "react-native-ui-lib": "^7.3.2",
    "react-native-unimodules": "^0.12.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-video": "^5.2.1",
    "react-native-video-controls": "^2.8.1",
    "react-native-video-player": "^0.14.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-indicators": "^0.16.2",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-native-typing-animation": "^0.1.0",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-native-video": "^5.0.14",
    "@types/react-test-renderer": "^18.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.5",
    "prettier": "^2.4.1",
